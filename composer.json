{
    "name": "autepos/ai-payment",
    "type": "project",
    "description": "Ai/love payment.",
    "keywords": [
        "payment"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "moneyphp/money": "^3.3",
        "stripe/stripe-php": "^7.58"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "orchestra/testbench": "^6.24",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Autepos\\AiPayment\\AiPaymentServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Autepos\\AiPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autepos\\AiPayment\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [

        ],
        "post-root-package-install": [
            
        ],
        "post-create-project-cmd": [

        ],
        "test":"php vendor/phpunit/phpunit/phpunit",
        "testf":"php vendor/phpunit/phpunit/phpunit --filter"
    }
}
